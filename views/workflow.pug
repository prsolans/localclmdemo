extends layout

block content
  div.row.main
    div.col-lg-4.col-sm-12
      h1.light Trigger a process
      form(name="add-estimation", method="post", enctype="application/x-www-form-urlencoded")
        div.input
            span.label Title
            input(type="text", name="title")
        div.input
            span.label Description
            textarea(name="description", rows="5")
        div.input
            input(type="submit", value="Submit")
    div.col-lg-8.col-sm-12
        iframe.document(width='100%', height='600', src=document)







